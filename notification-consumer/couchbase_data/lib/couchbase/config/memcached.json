{
  "active_external_users_push_interval": 600,
  "always_collect_trace_info": true,
  "audit_file": "/opt/couchbase/var/lib/couchbase/config/audit.json",
  "breakpad": {
    "enabled": true,
    "minidump_dir": "/opt/couchbase/var/lib/couchbase/crash"
  },
  "client_cert_auth": {
    "state": "disable",
    "prefixes": []
  },
  "collections_enabled": true,
  "connection_idle_time": 0,
  "connection_limit_mode": "disconnect",
  "datatype_snappy": true,
  "dedupe_nmvb_maps": false,
  "deployment_model": "default",
  "external_auth_service": false,
  "free_connection_pool_size": 0,
  "interfaces": [
    {
      "port": 11210,
      "host": "*",
      "ipv4": "required",
      "ipv6": "off"
    },
    {
      "port": 11209,
      "system": true,
      "host": "*",
      "ipv4": "required",
      "ipv6": "off"
    },
    {
      "port": 11207,
      "tls": true,
      "host": "*",
      "ipv4": "required",
      "ipv6": "off"
    },
    {
      "port": 11206,
      "system": true,
      "tls": true,
      "host": "*",
      "ipv4": "required",
      "ipv6": "off"
    },
    {
      "port": 11210,
      "host": "*",
      "ipv4": "off",
      "ipv6": "optional"
    },
    {
      "port": 11209,
      "system": true,
      "host": "*",
      "ipv4": "off",
      "ipv6": "optional"
    },
    {
      "port": 11207,
      "tls": true,
      "host": "*",
      "ipv4": "off",
      "ipv6": "optional"
    },
    {
      "port": 11206,
      "system": true,
      "tls": true,
      "host": "*",
      "ipv4": "off",
      "ipv6": "optional"
    }
  ],
  "logger": {
    "filename": "/opt/couchbase/var/lib/couchbase/logs/memcached.log",
    "cyclesize": 10485760
  },
  "max_client_connection_details": 0,
  "max_connections": 65000,
  "num_auxio_threads": "default",
  "num_nonio_threads": "default",
  "num_reader_threads": "default",
  "num_storage_threads": "default",
  "num_writer_threads": "default",
  "privilege_debug": false,
  "prometheus": {
    "port": 11280,
    "family": "inet"
  },
  "rbac_file": "/opt/couchbase/var/lib/couchbase/config/memcached.rbac",
  "root": "/opt/couchbase",
  "sasl_mechanisms": "SCRAM-SHA512 SCRAM-SHA256 SCRAM-SHA1 PLAIN",
  "scramsha_fallback_salt": "dmsjbkJHxcFVSgZR",
  "ssl_sasl_mechanisms": "SCRAM-SHA512 SCRAM-SHA256 SCRAM-SHA1 PLAIN",
  "system_connections": 5000,
  "tcp_keepalive_idle": 360,
  "tcp_keepalive_interval": 10,
  "tcp_keepalive_probes": 3,
  "tcp_user_timeout": 30,
  "tracing_enabled": true,
  "verbosity": 0,
  "xattr_enabled": true
}
